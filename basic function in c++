int sum(int a,int b){
    int sum=0;
    sum=a+b;
    return sum;
}
int digit_sum(int n){
    int k1=0;
    while(n){
    //   int  k1=0;
     k1=k1+n%10;
        n=n/10;
        // return k1;
    }
    return k1;
}
void reverse(int p){
    int p1=0;
    while(p){
        int u=p%10;
        p1=u+p1*10;
        p=p/10;
    }
    cout<<p1<<endl;
}
void increment(int &j){
    j++;
}
void sum1(int &a,int &b){
    cout<<a+b<<endl;
}
void func(string &r){
    string s="";
}
